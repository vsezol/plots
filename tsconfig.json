{
  "compilerOptions": {
    "target": "es5",
    "lib": [ "ESNext", "DOM"],
    "module": "CommonJS",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "#*": ["./*"],
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src", "./test"]
}
